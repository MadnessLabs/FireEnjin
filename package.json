{
  "name": "@enjin/fireenjin",
  "version": "1.0.3",
  "description": "A set of tools for speeding up production of full-stack Firebase projects.",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run clean && npm run build:types && npm run build:rollup",
    "build:rollup": "rollup -c",
    "build:types": "tsc --emitDeclarationOnly",
    "clean": "rimraf dist/",
    "g:input": "npm run plop input",
    "g:middleware": "npm run plop middleware",
    "g:migration": "npm run plop migration",
    "g:model": "npm run plop model",
    "g:query": "npm run plop query",
    "g:resolver": "npm run plop resolver",
    "g:seed": "npm run plop seed",
    "g:trigger": "npm run plop trigger",
    "g:unit": "npm run plop unit",
    "lint": "tslint --project tsconfig.json",
    "plop": "plop",
    "publish": "npm run build:types && npm run build:rollup",
    "semantic-release": "semantic-release",
    "serve": "rollup -c -w",
    "start": "npm run serve",
    "test": "jest --no-cache",
    "test:watch": "jest --watch --no-cache"
  },
  "bin": {
    "fireenjin": "dist/index.js"
  },
  "dependencies": {
    "@semantic-release/npm": "^5.3.2",
    "firebase-admin": "^8.6.0",
    "glob": "^7.1.4",
    "globby": "^10.0.1",
    "plop": "^2.5.2",
    "pluralize": "^8.0.0",
    "prettier": "^1.18.2",
    "yargs": "^14.2.0"
  },
  "devDependencies": {
    "@semantic-release/commit-analyzer": "^6.3.2",
    "@semantic-release/git": "^7.0.17",
    "@semantic-release/release-notes-generator": "^7.3.2",
    "@types/jest": "^24.0.18",
    "jest": "^24.9.0",
    "rimraf": "^3.0.0",
    "rollup": "^1.23.1",
    "rollup-plugin-typescript": "^1.0.1",
    "semantic-release": "^15.13.24",
    "ts-jest": "^24.1.0",
    "tslib": "^1.10.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MadnessLabs/FireEnjin.git"
  },
  "keywords": [
    "firebase",
    "github",
    "madness",
    "labs",
    "popcorn245",
    "youtube",
    "javascript",
    "typescript"
  ],
  "author": "Madness Labs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MadnessLabs/FireEnjin/issues"
  },
  "homepage": "https://github.com/MadnessLabs/FireEnjin#readme",
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(ts?)$",
    "testURL": "http://localhost",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
