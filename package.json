{
  "name": "@madnesslabs/fireenjin",
  "version": "1.1.7",
  "description": "A set of tools for speeding up production of full-stack Firebase projects.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rimraf dist/",
    "lint": "tslint --project tsconfig.json",
    "plop": "plop",
    "publish": "npm run build",
    "refresh": "rimraf ./node_modules ./package-lock.json && npm install",
    "semantic-release": "semantic-release",
    "serve": "tsc -w",
    "start": "npm run serve",
    "test": "jest --no-cache",
    "test:watch": "jest --watch --no-cache"
  },
  "bin": {
    "fireenjin": "dist/index.js"
  },
  "dependencies": {
    "firebase-admin": "^8.6.0",
    "glob": "^7.1.4",
    "globby": "^10.0.1",
    "plop": "^2.5.2",
    "pluralize": "^8.0.0",
    "prettier": "^1.18.2",
    "yargs": "^14.2.0"
  },
  "devDependencies": {
    "@semantic-release/commit-analyzer": "^6.3.2",
    "@semantic-release/git": "^7.0.17",
    "@semantic-release/npm": "^5.3.2",
    "@semantic-release/release-notes-generator": "^7.3.2",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.12.3",
    "jest": "^24.9.0",
    "rimraf": "^3.0.0",
    "semantic-release": "^15.13.24",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "tslib": "^1.10.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MadnessLabs/FireEnjin.git"
  },
  "keywords": [
    "firebase",
    "github",
    "madness",
    "labs",
    "popcorn245",
    "youtube",
    "javascript",
    "typescript"
  ],
  "author": "Madness Labs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MadnessLabs/FireEnjin/issues"
  },
  "homepage": "https://github.com/MadnessLabs/FireEnjin#readme",
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(ts?)$",
    "testURL": "http://localhost",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "private": false
}
