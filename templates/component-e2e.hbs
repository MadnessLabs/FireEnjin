import { newE2EPage } from "@stencil/core/testing";

describe("{{namespace}}{{nameFromPathDash name}}", () => {
  it("renders", async () => {
    const page = await newE2EPage();
    await page.setContent("<{{namespace}}{{nameFromPathDash name}}></{{namespace}}{{nameFromPathDash name}}>");

    const element = await page.find("{{namespace}}{{nameFromPathDash name}}");
    expect(element).toHaveClass("hydrated");
  });
});
